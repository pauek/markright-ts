@walk-test(1)
  @funcMap*
    [text]: (t) => t.toUpperCase()
  @input
    hi
  @output*
    HI
@walk-test(Walk test 1)
  @funcMap*
    a: (args, children) => `a(${args.join(", ")})[${children}]`,
  @input
    @a(1, 2){content}
  @output*
    a(1, 2)[content]

@walk-test(Text)
  @funcMap*
    [text]: (t) => t.toUpperCase(),
  @input
    hello
  @output*
    HELLO

@walk-test(InlineElement)
  @funcMap*
    elem: (args, children) => `[[${children.join("")}]]`,
  @input
    @elem{hi, there}
  @output*
    [[hi, there]]
